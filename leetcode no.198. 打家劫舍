class Solution(object):
    def rob(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        nums_sum1 = 0
        nums_sum2 = 0
        for i in range(0,len(nums),2):
            nums_sum1 += nums[i]
        for j in range(1,len(nums),2):
            nums_sum2 += nums[j]
        return max(nums_sum1,nums_sum2)
        
class Solution(object):
    def rob(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        last = 0
        now = 0
        for i in nums:
            last, now = now, max(last + i, now)
        return now 
        
